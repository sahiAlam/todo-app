<%- include("header"); -%>

    <div class="box" id="heading">
        <h1 id="heading">
            <%= listTitle %>
        </h1>
    </div>

    <div class="box">
        <% newItems.forEach((item)=> { %>
            <div class="item">
                <input type="checkbox">
                <p class="item">
                    <%= item.name %>
                </p>
            </div>
            <% }) %>
                <form class="item" action="/" method="post">
                    <input type="text" name="todoItem" placeholder="New Item" autocomplete="off">
                    <button type="submit">+</button>
                </form>
    </div>

    <%- include("footer"); -%>